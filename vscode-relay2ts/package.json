{
    "name": "vscode-relay2ts",
    "displayName": "vscode-relay2ts",
    "description": "Generates TypeScript interfaces for Relay fragments in the current source file.",
    "version": "0.0.1",
    "publisher": "alloy020",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.printPropsInterface",
        "onCommand:extension.printPropsInterfaceAtCursor"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.printPropsInterface",
                "title": "Relay: Print props interface at end or replace existing"
            },
            {
                "command": "extension.printPropsInterfaceAtCursor",
                "title": "Relay: Print props interface at cursor"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.32",
        "@types/node": "^6.0.40",
        "mocha": "^2.3.3",
        "typescript": "^2.0.3",
        "vscode": "^1.0.0"
    },
    "dependencies": {
        "graphql": "^0.8.2",
        "graphql-config-parser": "alloy/graphql-config#build",
        "relay2ts": "file:../relay2ts-0.1.0.tgz"
    }
}
